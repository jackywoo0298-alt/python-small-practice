foods = []
prices =[]
total = 0
while True:
    food = input("enter a food you want(q to quit): ").lower().strip()
    if food =="q":
        print("welcome to come next time!")
        break
    else:
        price = float(input(f"enter the price of {food}: "))
        foods.append(food)
        prices.append(price)
print("-----your cart-----")
for food in foods:
    print(food,end=" ")
for price in prices:
    total += price
print(f"your total costs is {total}cny")
